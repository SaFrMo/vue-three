{
    "name": "vue-three-wrap",
    "version": "1.1.01",
    "description": "Bare-bones Vue component wrapping a THREE.js instance.",
    "main": "./dist/vue-three-wrap.common.js",
    "scripts": {
        "dev": "vue serve ./example/App.vue",
        "build": "vue build --target lib --name vue-three-wrap ./src/VueThreeWrap.vue",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "three",
        "vue"
    ],
    "author": "Sander Moolin",
    "license": "MIT",
    "devDependencies": {
        "@vue/cli": "^3.2.1",
        "node-sass": "^4.11.0",
        "sass-loader": "^7.1.0"
    },
    "dependencies": {
        "fh-components": "^1.4.15",
        "three": "^0.99.0"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/safrmo/vue-three.git"
    },
    "files": [
        "dist/*",
        "src/*",
        "mixins/*",
        "example/*",
        "*.json",
        "*.js"
    ]
}
